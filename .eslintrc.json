{
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "single"],
        "quote-props": ["error", "as-needed"],
        "strict": ["error", "global"],
        "comma-dangle": ["error", "only-multiline"]
    }
}
